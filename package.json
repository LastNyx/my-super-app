{
  "name": "my-super-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "pnpm --parallel --filter \"./apps/*\" dev",
    "dev:api": "pnpm --filter @my-app/api dev",
    "dev:web": "pnpm --filter @my-app/web dev",
    "dev:web-solid": "pnpm --filter @my-app/web-solid dev",
    "build": "pnpm --filter \"./apps/*\" build",
    "db:generate": "pnpm --filter @my-app/api db:generate",
    "db:migrate": "pnpm --filter @my-app/api db:migrate",
    "db:studio": "pnpm --filter @my-app/api db:studio"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "prettier": "^3.6.2"
  }
}